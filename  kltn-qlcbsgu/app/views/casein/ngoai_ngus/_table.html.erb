<!-- Scaffolding generated by Casein v.3.1.11 -->

<table cellpadding="0" cellspacing="0" id="itemList" class="itemList">
	<tr>
		<th>Trinh do</th>
		<th>Ten ngoai ngu</th>
		<th>Can bo trinh do</th>
		<th>&nbsp;</th>
	</tr>
	
	<% ngoai_ngus.each_with_index do |ngoai_ngu, index| %>
		<tr class="<%= 'even' if index.modulo(2) == 0 %> hover">
			<td><%= casein_table_cell_link ngoai_ngu.trinh_do, casein_ngoai_ngu_path(ngoai_ngu) %></td>
			<td><%= casein_table_cell_link ngoai_ngu.ten_ngoai_ngu, casein_ngoai_ngu_path(ngoai_ngu) %></td>
			<td><%= casein_table_cell_link ngoai_ngu.can_bo_trinh_do, casein_ngoai_ngu_path(ngoai_ngu) %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("delete"), casein_ngoai_ngu_path(ngoai_ngu), :method => :delete, :confirm => "Are you sure you want to delete this ngoai ngu?") %>
			</td>
		</tr>
	<% end %>
</table>