<div class="blk-share">
<h2>Thêm mới ngoại ngữ</h2>

<%= form_for @ngoai_ngu, :url => casein_ngoai_ngus_path do |f| %>
	
	<%= render :partial => 'casein/ngoai_ngus/form', :locals => { :f => f } %>
	
	<p class="submits">
		<%= link_to 'Hủy', casein_ngoai_ngus_path, :class => 'cancel' %> hoặc
		<%= f.submit "Thêm mới ngoại ngữ", :class => "update" %>
