<h1>Nhập dữ liệu từ tập tin excel</h1>
<h2>Lựa chọn tập tin</h2>
<%= form_tag(parse_save_from_excel_casein_quyet_dinhs_path, :multipart => true) do %>

    <p>
      <%= file_field_tag 'excel_file' %>
    </p>
    <p>
      <%= submit_tag "Lưu dữ liệu", :disable_with => "Đang xử lý dữ liệu..." %>
    </p>
<% end %>

<%= content_for :sidebar do %>
    <li><%= link_to "#{casein_show_icon "add"}Thêm mới quyết định".html_safe, new_casein_quyet_dinh_path %></li>
    <li><%= link_to "#{casein_show_icon "table"}Danh sách quyết định".html_safe, casein_quyet_dinhs_path %></li>
<% end %>