	<tr>
		<th>Ten trinh do</th>
		<th>Mo ta</th>
		<th>&nbsp;</th>
	</tr>
	
	<% trinh_dos.each_with_index do |trinh_do, index| %>
		<tr class="<%= 'even' if index.modulo(2) == 0 %> hover">
			<td><%= casein_table_cell_link trinh_do.ten_trinh_do, casein_trinh_do_path(trinh_do) %></td>
			<td><%= casein_table_cell_link trinh_do.mo_ta, casein_trinh_do_path(trinh_do) %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("delete"), casein_trinh_do_path(trinh_do), :method => :delete, :confirm => "Are you sure you want to delete this trinh do?") %>
			</td>
		</tr>
	<% end %>
</table>