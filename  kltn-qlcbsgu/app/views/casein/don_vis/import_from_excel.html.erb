<div class="blk-share">
<h2>Nhập dữ liệu từ tập tin excel</h2>
<h2>Lựa chọn tập tin</h2>
<%= form_tag(parse_save_from_excel_casein_don_vis_path, :multipart => true) do %>

    <p>
      <%= file_field_tag 'excel_file' %>
    </p>
    <p>
      <%= submit_tag "Lưu dữ liệu", :disable_with => "Đang xử lý dữ liệu..." %>
    </p>
<% end %>
</div>

<%= content_for :sidebar do %>
    <li><%= link_to "#{casein_show_icon "add"}Thêm mới đơn vị".html_safe, new_casein_don_vi_path %></li>
    <li><%= link_to "#{casein_show_icon "table"}Danh sách đơn vị".html_safe, casein_don_vis_path %></li>
<% end %>