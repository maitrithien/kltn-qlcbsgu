<!-- Scaffolding generated by Casein v.3.1.11 -->
<div class="search_basic_form">
  <%= form_tag casein_can_bo_trinh_dos_path, :method => "get" do %>
      <%= text_field_tag "keyword", params["keyword"], :title => "Nhập vào Họ tên cán bộ/Mã cán bộ", :style => "padding: 2px;"%>
      <%= submit_tag "Tìm Kiếm",:disable_with => "Đang tìm kiếm...", :style => "padding: 3px 10px;" %>
  <% end %>
</div>

<h2>Danh sách trình độ cán bộ</h2>

<%= render :partial => 'casein/can_bo_trinh_dos/table', :locals => { :can_bo_trinh_dos => @can_bo_trinh_dos } %>

<%= will_paginate @can_bo_trinh_dos %>

<%= content_for :sidebar do %>
	<li><%= link_to "#{casein_show_icon "add"}Thêm mới trình độ cán bộ".html_safe, new_casein_can_bo_trinh_do_path %></li>
    <li><%= link_to "#{casein_show_icon('excel')}Xuất danh sách ra excel".html_safe, casein_can_bo_trinh_dos_path({:format => :xls, :keyword => params["keyword"], :commit => params["commit"], :utf8 => params["utf8"]}) %></li>
<% end %>