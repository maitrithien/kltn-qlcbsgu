<div class="blk-share">
<% unless @errors.nil? %>
    <h2>Danh sách các dòng không thể lưu vào cơ sở dữ liệu</h2>

    <table class="import_table">
      <tr>
        <th>STT dòng trong excel</th>
        <th>Cán bộ</th>
      </tr>
      <% @errors.each do |mgs| %>
          <tr>
            <td>Dòng&nbsp;<%= mgs[0]%>&nbsp;:</td>
            <td><%= mgs[1]%></td>
          </tr>
      <% end %>
    </table>
<% end %>
</div>

<%= content_for :sidebar do %>
    <li><%= link_to "#{casein_show_icon "add"}Thêm mới cán bộ".html_safe, new_casein_can_bo_cong_tac_path %></li>
    <li><%= link_to "#{casein_show_icon "import"}Thêm mới cán bộ từ tập tin excel".html_safe, import_from_excel_casein_can_bo_cong_tacs_path %></li>
<% end %>