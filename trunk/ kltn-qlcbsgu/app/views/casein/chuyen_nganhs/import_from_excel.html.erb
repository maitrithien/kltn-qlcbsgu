<h1>Nhập dữ liệu từ tệp tin excel</h1>
<h2>Lựa chọn tệp tin</h2>
<%= form_tag(parse_save_from_excel_casein_chuyen_nganhs_path, :multipart => true) do %>

<p>
  <%= file_field_tag 'excel_file' %>
</p>
<p>
  <%= submit_tag "Lưu dữ liệu" %>
</p>
<% end %>

<%= content_for :sidebar do %>
    <li><%= link_to "#{casein_show_icon "add"}Thêm mới chuyên ngành".html_safe, new_casein_chuyen_nganh_path %></li>
    <li><%= link_to "#{casein_show_icon "table"}Danh sách chuyên ngành".html_safe, casein_chuyen_nganhs_path %></li>
<% end %>