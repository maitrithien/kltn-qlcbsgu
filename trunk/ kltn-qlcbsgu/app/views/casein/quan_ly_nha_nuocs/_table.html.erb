			<td class="delete">
				<%= link_to(casein_show_row_icon("delete"), casein_quan_ly_nha_nuoc_path(quan_ly_nha_nuoc), :method => :delete, :confirm => "Bạn có chắc muốn xóa trình độ #{quan_ly_nha_nuoc.trinh_do}?") %>
			</td>
		</tr>
	<% end %>
</table>