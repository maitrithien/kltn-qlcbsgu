<div class="blk-share">
<h2>Cập nhật Lịch sử Ngạch - Bậc lương || Cán bộ <%= @lich_su_bac_luong.can_bo_thong_tin.ma_cb %></h2>

<%= form_for @lich_su_bac_luong, :url => casein_lich_su_bac_luong_path(@lich_su_bac_luong) do |f| %>

	<%= render :partial => 'casein/lich_su_bac_luongs/form', :locals => { :f => f } %>
	
	<p class="submits">
	  <%= link_to 'Trở về', casein_lich_su_bac_luongs_path, :class => 'cancel' %> hoặc
		<%= f.submit "Lưu thay đổi", :class => "update" %>
	</p>

<% end %>
</div>


<%= content_for :sidebar do %>
	<li><%= link_to "#{casein_show_icon "add"}Thêm mới lịch sử Ngạch - Bậc".html_safe, casein_lich_su_bac_luongs_path %></li>
	<li><%= link_to "#{casein_show_icon "import"}Thêm mới lịch sử Ngạch - Bậc từ tập tin excel".html_safe, import_from_excel_casein_lich_su_bac_luongs_path %></li>
	<li><%= link_to "#{casein_show_icon "table"}Danh sách lịch sử Ngạch - Bậc".html_safe, casein_lich_su_bac_luongs_path %></li>
<% end %>