<h1>Nhập dữ liệu từ tập tin excel</h1>
<h2>Lựa chọn tập tin</h2>
<%= form_tag(parse_save_from_excel_casein_lich_su_bac_luongs_path, :multipart => true) do %>

    <p>
      <%= file_field_tag 'excel_file' %>
    </p>
    <p>
      <%= submit_tag "Lưu dữ liệu" %>
    </p>
<% end %>

<%= content_for :sidebar do %>
    <li><%= link_to "#{casein_show_icon "add"}Thêm mới lịch sử lương".html_safe, new_casein_can_bo_li_lich_ct_path %></li>
    <li><%= link_to "#{casein_show_icon "table"}Danh sách lịch sử lương".html_safe, casein_can_bo_li_lich_cts_path %></li>
<% end %>