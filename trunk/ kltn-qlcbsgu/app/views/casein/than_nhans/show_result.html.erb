<div class="blk-share">
<% unless @errors.nil? %>
    <h2>Danh sách các dòng không thể lưu vào cơ sở dữ liệu</h2>
    <table class="import_table">
      <tr>
        <th>STT dòng</th>
        <th>Thông tin thân nhân</th>
      </tr>
      <% @errors.each do |mgs| %>
          <tr>
            <td>Dòng <%= mgs[0]%></td>
            <td><%= mgs[1] %></td>
          </tr>
      <% end %>

    </table>

<% end %>
</div>


<%= content_for :sidebar do %>
    <li><%= link_to "#{casein_show_icon "add"}Thêm mới thân nhân".html_safe, new_casein_than_nhan_path %></li>
    <li><%= link_to "#{casein_show_icon "add"}Thêm mới thân nhân từ file excel".html_safe, import_from_excel_casein_than_nhans_path %></li>
    <li><%= link_to "#{casein_show_icon "table"}Danh sách thân nhân cán bộ".html_safe, casein_than_nhans_path %></li>
<% end %>