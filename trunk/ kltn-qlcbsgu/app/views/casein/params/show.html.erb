<div class="blk-share">
<h2>Hiệu chỉnh tham số</h2>

<%= form_for @param, :url => casein_param_path(@param) do |f| %>

	<%= render :partial => 'casein/params/form', :locals => { :f => f } %>
	
	<p class="submits">
	  <%= link_to 'Hủy', casein_params_path, :class => 'cancel' %> hoặc
		<%= f.submit "Lưu thay đổi", :class => "update" %>
	</p>

<% end %>
</div>

<%= content_for :sidebar do %>
