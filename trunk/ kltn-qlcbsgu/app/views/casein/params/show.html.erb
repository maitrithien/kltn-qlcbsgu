        <li><%= link_to "#{casein_show_icon "add"}Thêm mới tham số từ file excel".html_safe, import_from_excel_casein_params_path %></li>
        <li><%= link_to "#{casein_show_icon "table"}Danh sách tham số".html_safe, casein_params_path %></li>
<% end %>