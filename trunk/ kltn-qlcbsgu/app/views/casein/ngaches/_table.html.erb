<!-- Scaffolding generated by Casein v.3.1.11 -->

<table cellpadding="0" cellspacing="0" id="itemList" class="itemList">
	<tr>
		<th>Mã ngạch</th>
		<th>Tên ngạch</th>
		<th>Niên hạn</th>
		<th>Ghi chú</th>
		<th>&nbsp;</th>
	</tr>
	
	<% ngaches.each_with_index do |ngach, index| %>
		<tr class="<%= 'even' if index.modulo(2) == 0 %> hover">
			<td><%= casein_table_cell_link ngach.ma_ngach, casein_ngach_path(ngach) %></td>
			<td><%= casein_table_cell_link ngach.ten_ngach, casein_ngach_path(ngach) %></td>
			<td><%= casein_table_cell_link ngach.nien_han, casein_ngach_path(ngach) %></td>
			<td><%= casein_table_cell_link ngach.ghi_chu, casein_ngach_path(ngach) %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("delete"), casein_ngach_path(ngach), :method => :delete, :confirm => "Bạn có chắc muốn xóa ngạch #{ngach.ma_ngach}?") %>
			</td>
		</tr>
	<% end %>
</table>