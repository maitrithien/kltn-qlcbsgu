<!-- Scaffolding generated by Casein v.3.1.11 -->
<div class="blk-share">
<h2>Thêm mới quyết định</h2>

<%= form_for @quyet_dinh, :url => casein_quyet_dinhs_path, :html => {:multipart => true} do |f| %>
	
	<%= render :partial => 'casein/quyet_dinhs/form', :locals => { :f => f } %>
	
	<p class="submits">
		<%= link_to 'Hủy', casein_quyet_dinhs_path, :class => 'cancel' %> hoặc
		<%= f.submit "Thêm mới quyết định", :class => "update" %>
	</p>

<% end %>
</div>


<%= content_for :sidebar do %>
    <li><%= link_to "#{casein_show_icon "add"}Thêm mới quyết định từ excel".html_safe, import_from_excel_casein_quyet_dinhs_path %></li>
	<li><%= link_to "#{casein_show_icon "table"}Danh sách quyết định".html_safe, casein_quyet_dinhs_path %></li>
<% end %>