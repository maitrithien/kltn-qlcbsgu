<!-- Scaffolding generated by Casein v.3.1.11 -->

<table cellpadding="0" cellspacing="0" id="itemList" class="itemList">
	<tr>
		<th>Cán Bộ</th>
		<th>Đơn Vị</th>
		<th>Chức Vụ</th>
		<th>Thời Gian Bắt Đầu</th>
		<th>Thời Gian Kết Thúc</th>
		<th>&nbsp;</th>
	</tr>
	
	<% qua_trinh_cong_tacs.each_with_index do |qua_trinh_cong_tac, index| %>
		<tr class="<%= 'even' if index.modulo(2) == 0 %> hover">
          <td><%= casein_table_cell_link "#{qua_trinh_cong_tac.can_bo_thong_tin.ma_cb} - #{qua_trinh_cong_tac.can_bo_thong_tin.ho_ten}", casein_qua_trinh_cong_tac_path(qua_trinh_cong_tac) %></td>
          <td><%= casein_table_cell_link "#{qua_trinh_cong_tac.don_vi.ten_don_vi}", casein_qua_trinh_cong_tac_path(qua_trinh_cong_tac) %></td>
          <td><%= casein_table_cell_link "#{qua_trinh_cong_tac.chuc_vu.ten_chuc_vu}", casein_qua_trinh_cong_tac_path(qua_trinh_cong_tac) %></td>
			<td><%= casein_table_cell_link qua_trinh_cong_tac.thoi_gian_bat_dau.strftime("%d/%m/%Y"), casein_qua_trinh_cong_tac_path(qua_trinh_cong_tac) %></td>
          <% if qua_trinh_cong_tac.thoi_gian_ket_thuc  %>
			<td><%= casein_table_cell_link qua_trinh_cong_tac.thoi_gian_ket_thuc.strftime("%d/%m/%Y"), casein_qua_trinh_cong_tac_path(qua_trinh_cong_tac) %></td>
           <% else %>
              <td><%= casein_table_cell_link qua_trinh_cong_tac.thoi_gian_ket_thuc, casein_qua_trinh_cong_tac_path(qua_trinh_cong_tac) %></td>
             <% end %>
			<td class="delete">
				<%= link_to(casein_show_row_icon("delete"), casein_qua_trinh_cong_tac_path(qua_trinh_cong_tac), :method => :delete, :confirm => "Are you sure you want to delete this qua trinh cong tac?") %>
			</td>
		</tr>
	<% end %>
</table>